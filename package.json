{
  "name": "mockforge",
  "version": "0.12.0-beta.0",
  "description": "",
  "type": "module",
  "files": [
    "json-schema",
    "dist"
  ],
  "typesVersions": {
    "*": {
      "vite-plugin": [
        "./dist/vite-plugin.d.ts"
      ],
      "webpack-plugin": [
        "./dist/webpack5-plugin.d.ts"
      ],
      "webpack5-plugin": [
        "./dist/webpack5-plugin.d.ts"
      ]
    }
  },
  "exports": {
    "./package.json": {
      "default": "./package.json"
    },
    "./webpack-plugin": {
      "types": "./dist/webpack5-plugin.d.ts",
      "require": "./dist/webpack5-plugin.cjs",
      "import": "./dist/webpack5-plugin.mjs"
    },
    "./webpack5-plugin": {
      "types": "./dist/webpack5-plugin.d.ts",
      "require": "./dist/webpack5-plugin.cjs",
      "import": "./dist/webpack5-plugin.mjs"
    },
    "./vite-plugin": {
      "types": "./dist/vite-plugin.d.ts",
      "require": "./dist/vite-plugin.cjs",
      "import": "./dist/vite-plugin.mjs"
    },
    "./server": {
      "types": "./dist/server/main.d.ts",
      "require": "./dist/server.cjs",
      "import": "./dist/server.mjs"
    }
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "repository": {
    "url": "https://github.com/MockForge/mockforge"
  },
  "scripts": {
    "prepublishOnly": "npm run build",
    "dev": "npm run dev:server & npm run dev:ui",
    "dev:ui": "vite",
    "dev:server": "tsx --watch scripts/start-server.ts",
    "build": "tsx scripts/build.ts && vite build",
    "cov": "vitest run --coverage src/test",
    "e2e": "vitest run src/e2e",
    "e2e:local": "npm run build && npm run e2e",
    "format": "prettier src scripts -w"
  },
  "author": "DiamondYuan",
  "license": "MIT",
  "packageManager": "pnpm@9.8.0+sha512.8e4c3550fb500e808dbc30bb0ce4dd1eb614e30b1c55245f211591ec2cdf9c611cabd34e1364b42f564bd54b3945ed0f49d61d1bbf2ec9bd74b866fcdc723276",
  "devDependencies": {
    "@ant-design/icons": "^5.4.0",
    "@types/cors": "^2.8.17",
    "@types/debug": "^4.1.12",
    "@types/express": "^4.17.21",
    "@types/json-stable-stringify": "^1.0.36",
    "@types/lodash-es": "^4.17.12",
    "@types/nise": "^1.4.5",
    "@types/node": "^22.5.4",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@types/ws": "^8.5.12",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/coverage-v8": "^2.0.5",
    "ahooks": "^3.8.1",
    "antd": "^5.20.5",
    "antd-style": "^3.7.0",
    "cors": "^2.8.5",
    "emittery": "^1.0.3",
    "esbuild": "^0.23.1",
    "fetch-mock": "^11.1.5",
    "get-port": "^7.1.0",
    "html-webpack-plugin": "^5.6.0",
    "immer": "^10.1.1",
    "json-stable-stringify": "^1.1.1",
    "lodash-es": "^4.17.21",
    "monaco-editor": "^0.51.0",
    "nanoid": "^5.0.7",
    "nise": "^6.0.0",
    "path-to-regexp": "^8.0.0",
    "playwright": "^1.48.0",
    "prettier": "^3.3.3",
    "query-string": "^9.1.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-monaco-editor": "^0.56.1",
    "strip-ansi": "^7.1.0",
    "tsx": "^4.19.0",
    "typescript": "^5.5.4",
    "valid-filename": "^4.0.0",
    "vite": "^5.4.3",
    "vitest": "^2.0.5",
    "zustand": "^4.5.5"
  },
  "dependencies": {
    "debug": "^4.3.7",
    "express": "^4.19.2",
    "ws": "^8.18.0"
  }
}
